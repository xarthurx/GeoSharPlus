namespace GeoBridgeFB;

struct Vec3 {
	x:double;
	y:double;
	z:double;
}

struct Int3 {
    x:int;
    y:int;
    z:int;
}

table Point3dData {
    vec: Vec3;
}
root_type Point3dData;

table PolylineData {
    vertices:[Vec3];
}
root_type PolylineData;

table MeshData {
    vertices:[Vec3];
    faces:[Int3];
}
root_type MeshData;